{
  "name": "@bitmovin/player-integration-yospace",
  "version": "1.2.25",
  "description": "Yospace integration for the Bitmovin Player",
  "main": "./dist/js/bitmovin-player-yospace.js",
  "types": "./dist/js/bitmovin-player-yospace.d.ts",
  "scripts": {
    "prepublishOnly": "gulp npm-prepare",
    "build": "gulp build-prod",
    "build-tv": "gulp build-prod && npm run copyToTizen && npm run copyToWebOS",
    "start": "gulp serve",
    "copyToTizen": "cp ./dist/js/bitmovin-player-yospace.min.js ./tizen/js",
    "copyToWebOS": "cp ./dist/js/bitmovin-player-yospace.min.js ./WebOS/src/js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitmovin/bitmovin-player-web-integrations-yospace"
  },
  "author": "Bitmovin",
  "license": "MIT",
  "dependencies": {
    "fast-safe-stringify": "^2.0.7"
  },
  "engines": {
    "node": "11.14.0"
  },
  "devDependencies": {
    "bitmovin-analytics": "^2.19.1",
    "bitmovin-player": "8.66.0",
    "bitmovin-player-ui": "^3.6.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
